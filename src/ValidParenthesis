static boolean isBalanced(String s) {
  Stack<Character> st = new Stack<>();
  for(int i=0; i<s.length; i++) {
      char ch = s.charAt(i);

      if(ch=='(' || ch=='{' || ch=='[') {
        st.push(ch);
      } else {
        if(!st.empty() && ((st.peek() == '(' && s.charAt(i) == ')') && (st.peek() == '{' && s.charAt(i) == '}') || (st.peek() == '[' && s.charAt(i) == ']')  )) {
          st.pop();
        } else {
          return false;
        }
      }
  }
  return st.empty;
}
